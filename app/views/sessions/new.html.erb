<div class="row">
  <div class="col-sm-6 col-sm-offset-1">
    <br />
    <br />
    <h1>Login</h1>
    <%= form_tag '/login' do %>
    <div class="form-group">
      <h4>Email: </h4>
      <%= text_field_tag :email %>
    </div>
    <div class="form-group">
      <h4>Password: </h4>
      <%= password_field_tag :password %>
    </div>
    <%= submit_tag "Submit" %>
    <% end %>
  </div>
</div>
